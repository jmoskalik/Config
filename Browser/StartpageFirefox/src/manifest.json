{
  "manifest_version": 2,
  "name": "CustomStartPage",
  "description": "CustomStartPage",
  "version": "0.2",
  "chrome_url_overrides": {
    "newtab": "CustomStartPage.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "assets/icons/favicon-16x16.png",
    "32": "assets/icons/favicon-32x32.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/favicon-16x16.png",
      "32": "assets/icons/favicon-32x32.png"
    }
  },
  "applications": {
    "gecko": {
      "id": "CustomStartPage@0.1",
      "strict_min_version": "60.0"
    }
  }
}