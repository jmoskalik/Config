{
  "manifest_version": 2,
  "name": "Custom StartPage",
  "description": "Startpage",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "48": "assets/icons/favicon-48x48.png",
    "96": "assets/icons/favicon-96x96.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "assets/icons/favicon-48x48.png",
      "96": "assets/icons/favicon-96x96.png"
    }
  },
  "applications": {
    "gecko": {
      "id": "x@x",
      "strict_min_version": "60.0"
    }
  }
}