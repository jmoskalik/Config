{
    "manifest_version": 2,
    "name": "Custom StartPage",
    "description": "Startpage",
    "version": "0.1",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "activeTab"
    ],
    "icons": {
        "16": "assets/icons/favicon-16x16.png",
        "32": "assets/icons/favicon-32x32.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/icons/favicon-16x16.png",
        "32": "assets/icons/favicon-32x32.png"
      }
    }
  }